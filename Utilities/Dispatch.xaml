x9;&#x9;&#x9;&#x9;status = &quot;Delayed&quot;&#xA;&#x9;&#x9;&#x9;ElseIf paymentDate &gt; today Then&#xA;&#x9;&#x9;&#x9;&#x9;status = &quot;On time&quot;&#xA;&#x9;&#x9;&#x9;Else&#xA;&#x9;&#x9;&#x9;&#x9;status = &quot;Due today&quot;&#xA;&#x9;&#x9;&#x9;End If&#xA;&#xA;&#x9;&#x9;&#x9;formattedTable.Rows.Add(&#xA;&#x9;&#x9;&#x9;&#x9;projectManager,&#xA;&#x9;&#x9;&#x9;&#x9;accountCode,&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of String)(&quot;שם חשבון&quot;),&#xA;&#x9;&#x9;&#x9;&#x9;subGroup.BudgetSection,&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of String)(&quot;סעיף תקציבי&quot;),&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of String)(&quot;אסמכתא&quot;),&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of Date)(&quot;תאריך אסמכתא&quot;),&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of Date)(&quot;תאריך לתשלום&quot;),&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of Date)(&quot;תאריך פעילות&quot;),&#xA;&#x9;&#x9;&#x9;&#x9;status,&#xA;&#x9;&#x9;&#x9;&#x9;row.Field(Of Decimal)(&quot;יתרה&quot;)&#xA;&#x9;&#x9;&#x9;)&#xA;&#xA;&#x9;&#x9;&#x9;groupTotal += row.Field(Of Decimal)(&quot;יתרה&quot;)&#xA;&#x9;&#x9;Next&#xA;&#x9;Next&#xA;&#xA;&#x9;' Add group total row&#xA;&#x9;formattedTable.Rows.Add(projectManager, accountCode, $&quot;{accountCode} Total&quot;, Nothing, Nothing, Nothing, Nothing, Nothing, Nothing, Nothing, groupTotal)&#xA;&#x9;grandTotal += groupTotal&#xA;Next&#xA;&#xA;' Add grand total row&#xA;formattedTable.Rows.Add(&quot;Grand Total&quot;, Nothing, Nothing, Nothing, Nothing, Nothing, Nothing, Nothing, Nothing, Nothing, grandTotal)&#xA;&#xA;' Display the formatted table (for testing purposes)&#xA;For Each row As DataRow In formattedTable.Rows&#xA;&#x9;Console.WriteLine(String.Join(&quot; | &quot;, row.ItemArray))&#xA;Next&#xA;" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="200,52" sap2010:WorkflowViewState.IdRef="InvokeCode_2">
